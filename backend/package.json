{
	"name": "agentstop-backend",
	"version": "0.0.1",
	"private": false,
	"license": "MIT",
	"author": "Educosys",
	"description": "Agentstop - The one-stop solution for building, managing, and deploying powerful AI agents and missions. Automate anything, faster.",
	"scripts": {
		"local": "cross-env NODE_ENV=local nest start --watch",
		"build": "cross-env NODE_ENV=local nest build",
		"start": "cross-env NODE_ENV=local node dist/main.js",
		"debug": "cross-env NODE_ENV=local nest start --debug --watch",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
